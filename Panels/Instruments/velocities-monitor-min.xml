<?xml version="1.0"?>

<!--

-->

<PropertyList>

  <name>Velocities Monitor - minimised</name>
  <w-base>110</w-base>
  <h-base>12</h-base>

  <layers>
<!-- Background -->
    <layer>
      <name>background</name>
      <texture>
        <path>Aircraft/MiG-15/Panels/Instruments/Textures/trans-dgrey-bg.rgb</path>
      </texture>
      <w>110</w>
      <h>12</h>
      <transformations>
        <transformation>
          <type>x-shift</type>
          <offset>0</offset>
        </transformation>
        <transformation>
          <type>y-shift</type>
          <offset>0</offset>
        </transformation>
      </transformations>
    </layer>

<!-- Title -->
    <layer>
      <name>Title</name>
      <type>text</type>
      <point-size>10</point-size>
      <color>
        <red>1</red>
        <green>1</green>
        <blue>0</blue>
      </color>
      <chunks>
        <chunk>
          <type>literal</type>
          <text>VEL Monitor      O</text>
        </chunk>
      </chunks>
      <transformations>
        <transformation>
          <type>x-shift</type>
          <offset>-53</offset>
        </transformation>
        <transformation>
          <type>y-shift</type>
          <offset>-3</offset>
        </transformation>
      </transformations>
    </layer>

  </layers>

  <actions>

    <action>
      <name>Maximise</name>
      <button>0</button>
      <x>35</x>
      <y>-4</y>
      <w>20</w>
      <h>9</h>
      <binding>
        <command>property-assign</command>
        <property>/instrumentation/velmonitor/minimised</property>
        <value type="string">false</value>
      </binding>
    </action>
    
  </actions>

</PropertyList>
